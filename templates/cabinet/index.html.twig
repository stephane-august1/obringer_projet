{% extends 'base.html.twig' %}

{% block title %}Hello CabinetController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 98%; width: 98%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1 class="text-center">Le Cabinet</h1>

    <h2 class="text-center">Cabinet de naturopathie et de réflexologie Mélanie Obringer</h2>

<h3 class="text-center">Vous êtes à la porte de la vitalité</h3>
 
<h5 class="text-center">Vidéo</h5>
<div class="row">
    <div class="col-lg-2 col-md-3 col-sm-2 col-xs-0"></div>
    <div class="col-lg-4 col-md-8 col-sm-10 col-xs-12">
    <div class="embedresize"><div>
        <iframe class="p-1" src="./images/video_cabinet.mp4" width="320" height="320" style="border:1px solid black;overflow:hidden" scrolling="no" frameborder="0" allowTransparency="true" allowFullScreen="true">
        </iframe></div>
        </div>
    </div>
    <div class="col-lg-1 col-md-3 col-sm-2 col-xs-0"></div>
    <div class="col-lg-4 col-md-8 col-sm-10 col-xs-12">
        <div class="embedresize">
            <iframe class="p-1" src="https://www.facebook.com/plugins/video.php?href=https%3A%2F%2Fwww.facebook.com%2FNaturopathemelanieobringer%2Fvideos%2F236072104138739%2F&show_text=0&width=476" width="320" height="320" style="border:1px solid black;overflow:hidden" scrolling="no" frameborder="0" allowTransparency="true" allowFullScreen="true">
            </iframe>
        </div>
    </div>
</div>
<div class="row">

{% for cabinet in cabinets %}
<div class="col-lg-4 ml-2s">



<img class="img-fluid " src="{{cabinet.imagesrc}}" alt="{{cabinet.imagesrc}}">

{{cabinet.name}}
    </div>
{% endfor %}

</div>
